{$DOMAIN:connect4.bschold.meme} {
  encode zstd gzip

  @backend path /api* /ws* /health
  reverse_proxy @backend server:3001

  reverse_proxy client:4173
}

{$STUDIO_DOMAIN:db.connect4.bschold.meme} {
  encode zstd gzip

  @backend path /api* /health
  reverse_proxy @backend server:3001

  redir / /studio 302
  reverse_proxy client:4173
}
